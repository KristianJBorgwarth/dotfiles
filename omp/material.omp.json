{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#C591E8>❯</><#69FF94>❯</>",
          "foreground": "#ffffff",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "agnoster_short"
          },
          "template": " {{ .Path }} ",
          "foreground": "#56B6C2",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#69FF94>❯</>",
          "foreground": "#ffffff",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none",
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "template": "<#5FAAE8>(</>{{ .HEAD }}<#5FAAE8>)</>",
          "foreground": "#c73ecc",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#69FF94>❯</>",
          "foreground": "#ffffff",
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04"
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#00cc74",
          "type": "time",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3
}
